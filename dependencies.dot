	cython -> build_essential;
	cython -> Cython;
	cython -> cython3;
	pplpy -> gmpy2;
	gmpy2 -> libgmp_dev;
	libgmp_dev -> libmpfr_dev;
	libgmp_dev -> libmpc_dev;
	libgmp_dev -> python_sphinx;
	{ rank=same; pyParser pyLPi pplpy gmpy2 }
	{ rank=same; cysignals cython libppl_dev}
		

}
