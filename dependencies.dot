digraph dependencies {
	
	rankdir=LR;
	/* My Modules */
	node [shape=box,style=filled,color=".7 .3 1.0"];
	pyRankFinder;
	pyLPi;
	pyParser;
	/* Python Modules */
	node [shape=ellipse,style=rounded,color="black"];
	"lark-parser";
	pydotplus;
	networkx;
	cysignals;
	z3;
	cython;
	// Special Python Modules 
	node [shape=doubleoctagon,style=rounded,color="red"];
	gmpy2;
	pplpy;

	// linux packages

	node [shape=triangle,style=rounded,color="black"];
	build_essential;
	cython3;
	Cython;
	libmpfr_dev;
	libmpc_dev;
	python_sphinx;
	libppl_dev;
	libgmp_dev;
		

	/* DEPENDENCIES */
